{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Orchestra.Agents.ClaudeCode": "Information",
      "Orchestra.API.Services.ClaudeAgentExecutor": "Information",
      "Orchestra.Core.AgentScheduler": "Warning",
      "Orchestra.Core.SimpleOrchestrator": "Warning",
      "Orchestra.API.Jobs.TaskExecutionJob": "Information"
    }
  },
  "AllowedHosts": "*",
  "SignalR": {
    "HubUrl": "http://localhost:55002/coordinatorHub",
    "FallbackUrl": "http://localhost:55002/coordinatorHub"
  },
  "Cors": {
    "BlazorOrigins": [
      "https://localhost:55004",
      "http://localhost:55001",
      "http://localhost:55002",
      "https://localhost:55003",
      "https://yourdomain.com"
    ],
    "AllowCredentials": true,
    "AllowedHeaders": ["Authorization", "Content-Type", "x-signalr-user-agent"]
  },
  "Cache": {
    "ChatSessionExpiry": "00:30:00",
    "MaxUserSessions": 100,
    "SizeLimit": 1000,
    "CompactionPercentage": 0.25
  },
  "ClaudeCode": {
    "DefaultCliPath": "C:\\Users\\mrred\\AppData\\Roaming\\npm\\claude.cmd",
    "DefaultTimeout": "00:10:00",
    "AllowedTools": [
      "Bash",
      "Read",
      "Write",
      "Edit",
      "Glob",
      "Grep",
      "MultiEdit",
      "NotebookEdit",
      "WebFetch",
      "WebSearch",
      "TodoWrite"
    ],
    "OutputFormat": "text",
    "MaxConcurrentExecutions": 3,
    "RetryAttempts": 2,
    "RetryDelay": "00:00:05",
    "WorkflowTimeout": "00:30:00",
    "WarmupEnabled": true,
    "WarmupDelayMs": 5000,
    "HealthCheckInterval": "00:05:00",
    "ProcessCleanupTimeout": "00:00:30"
  },
  "AgentHealthCheck": {
    "CheckInterval": "00:01:00",
    "HealthCheckTimeout": "00:00:30",
    "AgentInactivityThreshold": "00:05:00",
    "Enabled": true,
    "MaxRetryAttempts": 3,
    "RetryDelay": "00:00:05"
  },
  "AgentDiscovery": {
    "ScanInterval": "00:02:00",
    "StartupDelay": "00:00:30",
    "DiscoveryTimeout": "00:00:10",
    "Enabled": true,
    "ClaudeCodePorts": [ 3001, 3000, 8080, 55001 ],
    "AutoRegister": true,
    "ScanRepositories": [ "." ],
    "EnableProcessScanning": true,
    "EnableHttpScanning": true
  }
}
