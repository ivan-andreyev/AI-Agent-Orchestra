{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Orchestra.Agents.ClaudeCode": "Information",
      "Orchestra.API.Services.ClaudeAgentExecutor": "Information",
      "Orchestra.Core.AgentScheduler": "Warning",
      "Orchestra.Core.SimpleOrchestrator": "Warning",
      "Orchestra.API.Jobs.TaskExecutionJob": "Information"
    }
  },
  "AllowedHosts": "*",
  "SignalR": {
    "HubUrl": "http://localhost:55002/coordinatorHub",
    "FallbackUrl": "http://localhost:55002/coordinatorHub"
  },
  "Cors": {
    "BlazorOrigins": [
      "https://localhost:55004",
      "http://localhost:55001",
      "http://localhost:55002",
      "https://localhost:55003",
      "https://yourdomain.com"
    ],
    "AllowCredentials": true,
    "AllowedHeaders": ["Authorization", "Content-Type", "x-signalr-user-agent"]
  },
  "Cache": {
    "ChatSessionExpiry": "00:30:00",
    "MaxUserSessions": 100,
    "SizeLimit": 1000,
    "CompactionPercentage": 0.25
  },
  "ClaudeCode": {
    "DefaultCliPath": "C:\\Users\\mrred\\AppData\\Roaming\\npm\\claude.cmd",
    "DefaultTimeout": "00:10:00",
    "AllowedTools": [
      "Bash",
      "Read",
      "Write",
      "Edit",
      "Glob",
      "Grep",
      "MultiEdit",
      "NotebookEdit",
      "WebFetch",
      "WebSearch",
      "TodoWrite"
    ],
    "OutputFormat": "text",
    "MaxConcurrentExecutions": 3,
    "RetryAttempts": 2,
    "RetryDelay": "00:00:05",
    "WorkflowTimeout": "00:30:00",
    "WarmupEnabled": true,
    "WarmupDelayMs": 5000,
    "HealthCheckInterval": "00:05:00",
    "ProcessCleanupTimeout": "00:00:30"
  },
  "AgentHealthCheck": {
    "CheckInterval": "00:01:00",
    "HealthCheckTimeout": "00:00:30",
    "AgentInactivityThreshold": "00:05:00",
    "Enabled": true,
    "MaxRetryAttempts": 3,
    "RetryDelay": "00:00:05"
  },
  "AgentDiscovery": {
    "ScanInterval": "00:02:00",
    "StartupDelay": "00:00:30",
    "DiscoveryTimeout": "00:00:10",
    "Enabled": true,
    "ClaudeCodePorts": [ 3001, 3000, 8080, 55001 ],
    "AutoRegister": true,
    "ScanRepositories": [ "." ],
    "EnableProcessScanning": true,
    "EnableHttpScanning": true
  },
  "TerminalConnector": {
    "UseUnixSockets": true,
    "UseNamedPipes": true,
    "ConnectionTimeoutMs": 5000,
    "DefaultPipeName": null,
    "DefaultSocketPath": null
  },
  "TelegramEscalation": {
    "BotToken": "",
    "ChatId": "",
    "Enabled": false,
    "RequestTimeoutMs": 30000,
    "MaxRetries": 3
  },
  "ClaudeCodeSubprocess": {
    "TelegramRetry": {
      "MaxRetryAttempts": 3,
      "InitialDelayMs": 1000,
      "MaxDelayMs": 16000,
      "JitterEnabled": true,
      "RetryOn": [ 429, 500, 502, 503, 504 ]
    },
    "ApprovalTimeout": {
      "DefaultTimeoutMinutes": 30,
      "MaxTimeoutMinutes": 120,
      "CheckIntervalSeconds": 30,
      "GracePeriodSeconds": 0,
      "MaxConcurrentTimeouts": 10,
      "SendTimeoutWarnings": true,
      "WarningMinutesBeforeTimeout": 5
    }
  },
  "OpenTelemetry": {
    "Enabled": true,
    "ServiceName": "Orchestra.API",
    "ServiceVersion": "1.0.0",
    "MetricsEnabled": true,
    "TracingEnabled": false,
    "PrometheusEndpoint": "/metrics",
    "AspNetCoreInstrumentationEnabled": true,
    "HttpClientInstrumentationEnabled": true,
    "EntityFrameworkInstrumentationEnabled": false,
    "ExportIntervalSeconds": 60,
    "MaxMetricPoints": 10000,
    "DetailedMetricsEnabled": false
  }
}
